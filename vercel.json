{
  "functions": {
    "api/scenario.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/scenario",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, private, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "-1"
        },
        {
          "key": "X-Vercel-Cache",
          "value": "skip"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}